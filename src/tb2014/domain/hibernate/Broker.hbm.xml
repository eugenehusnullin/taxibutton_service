<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="tb2014.domain">
	<class name="Broker" table="brokers">
		<id name="id">
			<generator class="identity" />
		</id>
		<property name="uuid" type="java.lang.String" unique-key="broker_uk3" />
		<property name="apiId" length="20" unique-key="broker_uk1" />
		<property name="apiKey" length="20" unique-key="broker_uk2" />
		<property name="name" length="100" />
		<property name="apiurl" length="100" />
	</class>
</hibernate-mapping>