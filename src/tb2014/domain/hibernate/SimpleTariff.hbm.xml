<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="tb2014.domain.tariff">
	<class name="SimpleTariff" table="tariffs">
		<id name="id">
			<generator class="identity" />
		</id>
		<many-to-one name="broker" column="brokerid" unique-key="tariffs_uk1" />
		<property name="tariffs" length="10000"/>
	</class>
</hibernate-mapping>